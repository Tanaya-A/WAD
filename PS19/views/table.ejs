<!DOCTYPE html>
<html>
<head>
    <title>Student Marks Table</title>
    <style>
        table { border-collapse: collapse; width: 80%; margin: 20px auto; }
        th, td { border: 1px solid black; padding: 10px; text-align: center; }
    </style>
</head>
<body>
    <h2 style="text-align: center;">Student Marks Table</h2>
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Roll No</th>
                <th>WAD</th>
                <th>DSBDA</th>
                <th>CNS</th>
                <th>CC</th>
                <th>AI</th>
            </tr>
        </thead>
        <tbody>
            <% students.forEach(s => { %>
                <tr>
                    <td><%= s.Name %></td>
                    <td><%= s.Roll_No %></td>
                    <td><%= s.WAD_Marks %></td>
                    <td><%= s.DSBDA_Marks %></td>
                    <td><%= s.CNS_Marks %></td>
                    <td><%= s.CC_Marks %></td>
                    <td><%= s.AI_Marks %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
