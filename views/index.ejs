<!DOCTYPE html>
<html>
<head>
    <title>Song List</title>
    <style>
        table {
            width: 80%;
            border-collapse: collapse;
            margin: 30px auto;
        }
        th, td {
            border: 1px solid #999;
            padding: 10px;
            text-align: center;
        }
        h2 {
            text-align: center;
        }
    </style>
</head>
<body>
    <h2>Total Songs: <%= count %></h2>
    <table>
        <tr>
            <th>Song Name</th>
            <th>Film</th>
            <th>Music Director</th>
            <th>Singer</th>
            <th>Actor</th>
            <th>Actress</th>
        </tr>
        <% songs.forEach(song => { %>
            <tr>
                <td><%= song.Songname %></td>
                <td><%= song.Film %></td>
                <td><%= song.Music_director %></td>
                <td><%= song.Singer %></td>
                <td><%= song.Actor || "-" %></td>
                <td><%= song.Actress || "-" %></td>
            </tr>
        <% }) %>
    </table>
</body>
</html>
